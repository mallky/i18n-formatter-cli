{
  "name": "i18n-formatter-cli",
  "version": "0.1.1",
  "description": "i18n automatization cli tool. Easily add keys, merge translation into an existing file, and create new locale files.",
  "keywords": [
    "i18n",
    "cli-tool",
    "internationalization"
  ],
  "main": "bundle.js",
  "bin": {
    "i18n-cli": "./bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mallky/i18n-formatter-cli.git"
  },
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "testAddAll": "npm run build && i18n-cli addAllKeys",
    "testMerge": "npm run build && i18n-cli merge jp-jp.json",
    "testCreate": "npm run build && i18n-cli create ua-ua.json",
    "build:cli": "rollup --config rollup.config.js",

    "dev:ui": "cd ui-server && next dev",
    "start:ui": "cd ui-server && next start",
    "lint:ui": "cd ui-server && next lint",
    "build:ui": "cd ui-server && next build"
  },
  "dependencies": {
    "cli-color": "2.0.1",
    "commander": "9.0.0",
    "lodash.clonedeep": "^4.5.0",
    "next": "12.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "recoil": "^0.5.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "17.0.21",
    "@types/react": "17.0.40",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "27.5.1",
    "prettier": "^2.5.1",
    "rollup": "^2.69.2",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "4.6.2"
  },
  "author": "Makar Kuzmichev (https://github.com/mallky)",
  "license": "MIT"
}